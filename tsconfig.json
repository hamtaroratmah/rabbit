{
  "extends": ".expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-native",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@env": ["node_modules/react-native-dotenv"]
    },
    "allowSyntheticDefaultImports": true,
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true
  }
}
